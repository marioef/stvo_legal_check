@prefix sys:       <http://www.semanticweb.org/mario/ontologies/2021/6/untitled-ontology-9#> .
@prefix rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:       <http://www.w3.org/2002/07/owl#> .

[parking_forbidden: (?lane rdf:type ?laneType), (?laneType rdfs:subClassOf sys:Lane), (?car rdf:type ?carType), (?carType rdfs:subClassOf sys:Participants), (?lane sys:hasParticipant ?car), (?car sys:isParking ?lane),  listEqual(?laneType,sys:OneWayLane), print(?car,?lane) -> (?car sys:Situation "not legal"), print("parking_forbidden fired")]

[rightRule: (?car1 sys:isOn ?lane1), (?lane1 sys:turnRightTo ?laneRight), (?car2 sys:isOn ?lane2), (?lane2 sys:isLaneOf ?lane2All), (?laneRight sys:isLaneOf ?lane2All), (?car1 sys:driveTo ?dest1), (?dest1 sys:isLaneOf ?dest1Lane), notEqual(?dest1Lane,?lane2All), (?car1 sys:isDriving ?current_street), (?car2 sys:isWaiting ?car1), (?lane1 sys:hasSign ?lane1Sign), (?lane2 sys:hasSign ?lane2Sign), (?lane1Sign rdf:type ?lane1SignClass), (?lane2Sign rdf:type ?lane2SignClass), (?lane1SignClass rdfs:subClassOf sys:PrioritySigns), (?lane2SignClass rdfs:subClassOf sys:PrioritySigns), equal(?lane1SignClass,?lane2SignClass) -> (?car1 sys:Situation "not legal"), print("rightRule fired")]
